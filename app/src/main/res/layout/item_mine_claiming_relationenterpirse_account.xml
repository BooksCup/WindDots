<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="75dp"
    android:orientation="horizontal"
   >

    <ImageView
        android:layout_marginHorizontal="15dp"
        android:id="@+id/item_usersel_icon"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:src="@mipmap/unselect"
        android:layout_gravity="center_vertical"
        />
    <LinearLayout
        android:layout_width="0dp"
        android:layout_weight="1"
        android:layout_height="wrap_content"
        android:orientation="vertical">
        <TextView
            android:id="@+id/item_mine_claiming_enterpriseaccount_name"
            android:layout_width="match_parent"
            android:layout_height="25dp"
            android:text="name"
            android:gravity="center_vertical"
            android:textColor="@color/color32"
            android:textSize="15sp" />

        <TextView
            android:id="@+id/item_mine_claiming_enterpriseaccount_bank"
            android:layout_width="match_parent"
            android:layout_height="25dp"
            android:gravity="center_vertical"
            android:text="name"
            android:textColor="@color/color50"
            android:textSize="12sp" />

        <TextView
            android:id="@+id/item_mine_claiming_enterpriseaccount_number"
            android:layout_width="match_parent"
            android:layout_height="25dp"
            android:text="name"
            android:textColor="@color/color50"
            android:gravity="center_vertical"
            android:textSize="12sp" />
    </LinearLayout>


</LinearLayout>
