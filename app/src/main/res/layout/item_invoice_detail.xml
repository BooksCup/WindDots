<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <com.wd.winddots.desktop.list.invoice.view.InvoiceDetailCell
        android:id="@+id/cell_goodserviceName"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="1dp"/>
    <com.wd.winddots.desktop.list.invoice.view.InvoiceDetailCell
        android:id="@+id/cell_model"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="1dp"/>
    <com.wd.winddots.desktop.list.invoice.view.InvoiceDetailCell
        android:id="@+id/cell_unit"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="1dp"/>
    <com.wd.winddots.desktop.list.invoice.view.InvoiceDetailCell
        android:id="@+id/cell_numbers"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="1dp"/>
    <com.wd.winddots.desktop.list.invoice.view.InvoiceDetailCell
        android:id="@+id/cell_price"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="1dp"/>
    <com.wd.winddots.desktop.list.invoice.view.InvoiceDetailCell
        android:id="@+id/cell_tax_sums"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="1dp"/>
    <com.wd.winddots.desktop.list.invoice.view.InvoiceDetailCell
        android:id="@+id/cell_taxRate"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="1dp"/>
    <com.wd.winddots.desktop.list.invoice.view.InvoiceDetailCell
        android:id="@+id/cell_tax_tax"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="1dp"
        android:layout_marginBottom="10dp"
        />

</LinearLayout>
